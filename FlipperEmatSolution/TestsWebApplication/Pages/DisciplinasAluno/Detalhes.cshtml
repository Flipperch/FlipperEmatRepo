@page
@model TestsWebApplication.Pages.DisciplinasAluno.DetalhesModel
@{
    ViewData["Title"] = "Detalhes Disciplina Aluno";
}

<h1>@ViewData["Title"]</h1>

<div class="container">
    <p>
        <a asp-page="./Index">Retornar</a>
    </p>
    <div>
        <h4>Disciplina Aluno</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.DisciplinaAlunoId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DisciplinaAluno.DisciplinaAlunoId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.MatriculaId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DisciplinaAluno.MatriculaId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.DisciplinaId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DisciplinaAluno.DisciplinaId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.Atual)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DisciplinaAluno.Atual)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.Concluida)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DisciplinaAluno.Concluida)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DisciplinaAluno.Atendimentos)
            </dt>
            <dd class="col-sm-10">
                <table class="table">
                    <thead>
                        <tr>
                            <th>AtendimentoId</th>
                            <th>DisciplinaAlunoId</th>
                            <th>TipoAtendimentoId</th>
                            <th>ProfessorId</th>
                            <th>DataAtendimento</th>
                            <th>ProfessorAlteracaoId</th>
                            <th>DataAlteracao</th>
                            <th>Modulo</th>
                            <th>Ordem</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.DisciplinaAluno.Atendimentos)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AtendimentoId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DisciplinaAlunoId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TipoAtendimentoId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProfessorId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DataAtendimento)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProfessorAlteracaoId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DataAlteracao)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Modulo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Ordem)
                                </td>
                                <td>
                                    <a asp-page=".Atendimentos/Edit" asp-route-id="@item.AtendimentoId">Alterar</a> |
                                    <a asp-page=".Atendimentos/Detalhes" asp-route-id="@item.AtendimentoId">Detalhes</a> |
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </dd>
        </dl>
    </div>
</div>
